{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    },
    "ghana-emr-openmrs": {
      "command": "node",
      "args": ["/workspaces/MedReg/mcp-servers/openmrs/dist/index.js"],
      "env": {
        "OPENMRS_BASE_URL": "http://localhost:8080/openmrs/ws/rest/v1",
        "OPENMRS_USERNAME": "admin",
        "OPENMRS_PASSWORD": "Admin123",
        "FACILITY_CODE": "KBTH",
        "REGION_CODE": "GA"
      }
    },
    "ghana-emr-mysql": {
      "command": "node",
      "args": ["/workspaces/MedReg/mcp-servers/mysql/dist/index.js"],
      "env": {
        "MYSQL_HOST": "localhost",
        "MYSQL_PORT": "3307",
        "MYSQL_USER": "openmrs_user",
        "MYSQL_PASSWORD": "openmrs_password",
        "MYSQL_DATABASE": "openmrs"
      }
    }
  }
}